{
  "schemaVersion": 1,
  "id": "rubymod",
  "version": "${version}",

  "name": "RubyMod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/TheOnlyTails/RubyModFabric",
    "sources": "https://github.com/TheOnlyTails/RubyModFabric"
  },

  "license": "CC0-1.0",
  "icon": "assets/rubymod/icon.png",
